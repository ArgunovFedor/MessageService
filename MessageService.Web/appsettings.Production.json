{
  "App": {
    "Issuer": "https://auth-server-api.kube.aebank.lan/"
  },
  "Auth": {
    "Api": {
      "Issuer": "https://auth-server-api.kube.aebank.lan/"
      // ToDo: Add Secret in Vault APP_Auth__Api__Secret
    },
    "Resource": {
      "Issuer": "https://auth-server-api.kube.aebank.lan/"
    }
  },
  "LoginEndpoint": {
    "AuthServer": {
      // ToDo: Add Secret in Vault APP_LoginEndpoint__AuthServer__Secret
      "Issuer": "https://auth-server-api.kube.aebank.lan/",
      "SignInOidcUrl": "https://message-service-api.kube.aebank.lan/api/Authorization/signin-oidc"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": {
      "Elasticsearch": {
        "Args": {
          "indexFormat": "aeb-production-{0:yyyy-MM-dd}"
        }
      }
    }
  }
}
