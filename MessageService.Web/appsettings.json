{
  "AllowedHosts": "*",
  "App": {
    "Database": {
      "DatabaseEngine": "PostgreSql",
      "Host": "localhost",
      "Database": "MessageService",
      "Username": "postgres",
      "Password": "Qwerty123!"
    }
  },
  "JAEGER_SERVICE_NAME": "MessageService",
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "Serilog": {
    "Properties": {
      "ServiceName": "MessageService"
    },
    "MinimumLevel": {
      "Default": "Debug"
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "RequestPath like '/health%' or EndsWith(RequestPath, '/metrics')"
        }
      },
      {
        "Name": "ByExcluding",
        "Args": {
          "expression": "SourceContext like 'Microsoft.Extensions.Diagnostics.HealthChecks%'"
        }
      }
    ],
    "WriteTo": {
      "Console": {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:} [{Level}]|{SourceContext}| {Message}{NewLine}{Exception}"
        }
      },
      "Elasticsearch": {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "sample-application-web-local-{0:yyyy-MM}",
          "restrictedToMinimumLevel": "Verbose",
          "emitEventFailure": "WriteToSelfLog",
          "autoRegisterTemplate": true,
          "autoRegisterTemplateVersion": "ESv7",
          "customFormatter": "Aeb.DigitalPlatform.Infrastructure.ElasticSearchCustomFormatter,Aeb.DigitalPlatform.Infrastructure.ElasticSearchSink"
        }
      },
      "Sentry": {
        "Name": "Sentry",
        "Args": {
          "MaxRequestBodySize": "Always",
          "IncludeRequestPayload": true,
          "MinimumBreadcrumbLevel": "Verbose",
          "MinimumEventLevel": "Warning",
          "AttachStackTrace": true,
          "InitializeSdk": true
        }
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  },
  "Sentry": {
    "Dsn": "http://dns-example",
    "MaxRequestBodySize": "Always",
    "IncludeRequestPayload": true,
    "SendDefaultPii": true,
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": true,
    "DiagnosticsLevel": "Error"
  },
  "WebSocketServer": {
    "Url": "ws://localhost:6969/ws"
  }
}
